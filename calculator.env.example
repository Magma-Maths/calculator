# Magma execution
MAGMA_TIMEOUT=120
MAGMA_CPU_TIMEOUT=120
MAGMA_MEMORY_MB=400
MAGMA_INPUT_KB=50
MAGMA_OUTPUT_KB=20

# Service
MAX_CONCURRENT=4
PORT=8080

# Rate limiting
RATE_LIMIT_PER_MINUTE=30
RATE_LIMIT_PER_HOUR=200

# CORS
ALLOWED_ORIGIN=*

# Usage logging
USAGE_LOG_FILE=/data/usage.jsonl

# Optional Turnstile
TURNSTILE_ENABLED=false
TURNSTILE_SECRET_KEY=
